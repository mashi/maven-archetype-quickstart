{
  "extends": [
    "config:base",
    ":semanticCommits",
    ":semanticCommitScopeDisabled",
    ":semanticCommitTypeAll(chore)"
  ],
  "github-actions": {
    "fileMatch": ["renovate.json"]
  },
  "dependencyDashboard": true,
  "branchConcurrentLimit": 50,
  "prHourlyLimit": 0,
  "prConcurrentLimit": 50,
  "automerge": true,
  "automergeType": "branch"
}
